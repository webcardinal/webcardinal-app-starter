{
  "name": "cardinal-app-starter",
  "version": "1.0.0",
  "description": "Application template that combines multiple components of Cardinal.",
  "scripts": {
    "postinstall": "node ./node_modules/octopus/scripts/run && npm run build-cardinal",
    "build-cardinal": "npm run build-cardinal-components && npm run build-cardinal-themes",
    "build-cardinal-themes": "node ./node_modules/octopus/scripts/run build-cardinal-themes",
    "build-cardinal-components": "node ./node_modules/octopus/scripts/run build-cardinal-components",
    "server": "ws --spa ./index.html"
  },
  "dependencies": {},
  "devDependencies": {
    "local-web-server": "^4.2.1",
    "octopus": "git+https://github.com/webcardinal/octopus"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webcardinal/cardinal-app-starter.git"
  },
  "license": "MIT"
}
